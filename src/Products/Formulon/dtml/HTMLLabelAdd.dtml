<html>
 <head>
  <title>Add Label</title>
 </head>

  <body bgcolor="#FFFFFF" link="#000099" vlink="#555555" alink="#77003B">

  <form action="manage_addHTMLLabel" method="POST">
   <table cellspacing="2">
    <tr bgcolor="#DDDDDD"><td colspan="4"><h2>Add Label</h2></td></tr>
    <tr><td colspan="4">&nbsp;</td></tr>

    <tr>
     <td> Id: </td>
     <td> <input type="TEXT" name="id:string" size="25"> </td>
    </tr>
    
    <tr>
     <td> Content: </td>
     <td colspan="3"> <input type="TEXT" name="value:string" size="100"> </td>
    </tr>

    <tr>
     <td> Stylesheet class: </td>
     <td colspan="3"> <input type="TEXT" name="user_css:string" size="100"> </td>
    </tr>
    
    <tr><td colspan="4">&nbsp;</td></tr>
    <tr><td colspan="4"><h3>Positioning:</h3></th></tr>

    <tr>
     <td> Column: </td>
     <td> <input type="TEXT" name="column:int" size="25" value="1"> </td>
     <td> Column spanning: </td>
     <td> <input type="TEXT" name="colspan:int" size="25" value="-1"> </td>
    </tr>
    
    <tr>
     <td> Row: </td>
     <td> <input type="TEXT" name="row:int" size="25" value="1"> </td>
    </tr>

    <tr>
     <td><br><input type="SUBMIT" value="Add"></td>
    </tr>
   </table>
  </form>
 </body>
</html>
